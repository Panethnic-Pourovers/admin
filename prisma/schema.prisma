
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Book {
    id              String           @default(cuid()) @id
    barcodeId       String
    title           String
    copies          Int
    members         LibraryMember[]  
}

model LibraryMember {
    id                String     @default(cuid()) @id
    firstName         String
    lastName          String 
    email             String
    phone             String?
    checkedOut        Book[]    
    history           String     
}